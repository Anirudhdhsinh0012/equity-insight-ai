# Twilio WhatsApp API Configuration
NEXT_PUBLIC_TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
NEXT_PUBLIC_TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
